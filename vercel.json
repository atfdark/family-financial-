{
  "version": 2,
  "functions": {
    "api/index.py": {
      "runtime": "@vercel/python",
      "maxDuration": 30
    }
  },
  "regions": ["iad1"],
  "build": {
    "env": {
      "PYTHON_VERSION": "3.10"
    }
  },
  "installCommand": "pip install -r requirements.txt",
  "buildCommand": "echo 'Build completed successfully'",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ],
  "env": {
    "PYTHONPATH": "$PYTHONPATH:/var/task"
  }
}